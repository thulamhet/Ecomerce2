{
  "name": "ecom",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "npx react-native run-ios --simulator='iPhone 13 Pro Max'",
    "ipad": "npx react-native run-ios --simulator='iPad Pro (11-inch) (3rd generation)'",
    "iphonese": "npx react-native run-ios --simulator='iPhone SE (2nd generation)'",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "clean:ios": "cd ios && xcodebuild clean && cd ../",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/EofficeClone/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "env:prod": "ENV=production ./scripts/switch_env.sh",
    "env:test": "ENV=dev ./scripts/switch_env.sh",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@eva-design/eva": "^2.1.0",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-firebase/app": "^12.9.3",
    "@react-native-firebase/database": "^12.9.3",
    "@react-navigation/bottom-tabs": "^6.0.9",
    "@react-navigation/material-top-tabs": "^6.0.6",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.4",
    "@reduxjs/toolkit": "^1.6.2",
    "@ui-kitten/components": "5.1.1",
    "@ui-kitten/eva-icons": "^5.1.1",
    "@ui-kitten/metro-config": "5.1.1",
    "axios": "^0.23.0",
    "native-base": "^3.2.2",
    "react": "17.0.1",
    "react-native": "0.64.1",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-pager-view": "^5.4.9",
    "react-native-reanimated": "^2.2.3",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.8.0",
    "react-native-svg": "^12.1.1",
    "react-native-tab-view": "^3.1.1",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "styled-components": "^5.3.3",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.64.5",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^26.6.3",
    "eslint": "^7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "react-test-renderer": "17.0.1",
    "redux-devtools": "^3.7.0",
    "typescript": "^3.8.3"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
